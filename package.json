{
  "name": "@superdispatch/actions",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "workspaces": {
    "packages": [
      "*"
    ]
  },
  "scripts": {
    "build": "lerna run version --parallel",
    "postinstall": "husky install",
    "lint": "js-tools lint",
    "prerelease": "yarn install --frozen-lockfile && yarn tsc && yarn lint",
    "release": "yarn lerna version",
    "postrelease": "yarn ts-node scripts/set-latest-tags.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*": "js-tools lint --fix"
  },
  "prettier": "@superdispatch/prettier-config",
  "devDependencies": {
    "@actions/cache": "^1.0.6",
    "@actions/core": "^1.2.6",
    "@actions/exec": "^1.0.4",
    "@actions/github": "^4.0.0",
    "@actions/glob": "^0.1.1",
    "@superdispatch/eslint-plugin": "^0.3.0",
    "@superdispatch/js-tools": "^0.3.0",
    "@superdispatch/prettier-config": "^0.3.0",
    "@superdispatch/tsconfig": "^0.3.0",
    "@types/pretty-bytes": "^5.2.0",
    "esbuild": "^0.8.44",
    "eslint": "^7.19.0",
    "husky": "^5.0.9",
    "lerna": "^3.22.1",
    "lint-staged": "^10.5.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "pretty-bytes": "^5.5.0",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.5",
    "zlib": "^1.0.5"
  },
  "engines": {
    "node": ">=12"
  }
}
